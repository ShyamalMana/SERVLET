

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
  <welcome-file>Signup.html</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>oneservlet</servlet-name>
  <servlet-class>com.controller.Signup_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>oneservlet</servlet-name>
  <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>twoservlet</servlet-name>
  <servlet-class>com.controller.Login_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
   <servlet-name>twoservlet</servlet-name>
   <url-pattern>/Login</url-pattern>
  </servlet-mapping>
</web-app>
